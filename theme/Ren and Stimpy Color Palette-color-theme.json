  "editor.tokenColorCustomizations": {
    "textMateRules": [
      // General
      {
        "name": "Strings",
        "scope": [
          "string.quoted.double",
          "string.quoted.double punctuation",
          "string.quoted.single",
          "punctuation.definition.string.template.begin.js",
          "punctuation.definition.string.template.end.js",
          "string.template.js"
        ],
        "settings": {
          "foreground": "#80aef3"
        }
      },
      {
        "name": "Operators",
        "scope": [
          "keyword.operator.arithmetic.js",
          "keyword.operator.comparison.js",
          "keyword.operator.assignment.js",
          "keyword.operator.logical.js"
        ],
        "settings": {
          "foreground": "#d2a1ff",
        }
      },
      // HTML Colors
      {
        "name": "[HTML] Comment and Punctuation Settings",
        "scope": [
          "comment",
          "punctuation.definition.tag.begin.html",
          "punctuation.definition.tag.end.html"
        ],
        "settings": {
          "foreground": "#ffd0ead3",
          "fontStyle": "italic"
        }
      },
      {
        "name": "[HTML] Entity Tag Settings",
        "scope": ["entity.name.tag.html"],
        "settings": {
          "foreground": "#ec635f"
        }
      },
      {
        "name": "[HTML] Tag Attributes Settings",
        "scope": ["entity.other.attribute-name"],
        "settings": {
          "foreground": "#F0E0C0",
          "fontStyle": "italic"
        }
      },
      // JSON Colors
      {
        "name": "[JSON] Variable type",
        "scope": ["meta.var.expr.js"],
        "settings": {
          "foreground": "#F0E0C0"
        }
      },
      {
        "name": "[JSON] Properties",
        "scope": [
          "support.type.property-name.json",
          "support.type.property-name.json punctuation"
        ],
        "settings": {
          "foreground": "#a480f3"
        }
      },
      // Javascript Colors
      {
        "name": "[JAVASCRIPT] Type",
        "scope": ["storage.type.js"],
        "settings": {
          "foreground": "#EB71B6"
        }
      },
      {
        "name": "[JAVASCRIPT] Variable type",
        "scope": [
          "storage.type.js",
          "support.class.component.js",
          "entity.name.tag.js"
        ],
        "settings": {
          "foreground": "#f9c0c6"
        }
      },
      {
        "name": "[JAVASCRIPT] Variable declared",
        "scope": ["variable.other.constant.js"],
        "settings": {
          "foreground": "#EB71B6"
        }
      },
      {
        "name": "[JAVASCRIPT] Support function",
        "scope": [
          "support.function.js",
          "keyword.control.export.js"
        ],
        "settings": {
          "foreground": "#e6d0c5"
        }
      },
      {
        "name": "[JAVASCRIPT] Function",
        "scope": ["storage.type.function.js"],
        "settings": {
          "foreground": "#afa568",
          "fontStyle": "italic"
        }
      },
      {
        "name": "[JAVASCRIPT] Name of function",
        "scope": ["entity.name.function.js"],
        "settings": {
          "foreground": "#e4d4c5"
        }
      },
      {
        "name": "[JAVASCRIPT] Return function",
        "scope": ["keyword.control.flow.js"],
        "settings": {
          "foreground": "#97b99e"
        }
      },
      {
        "name": "[JAVASCRIPT] Module export",
        "scope": ["support.type.object.module.js"],
        "settings": {
          "foreground": "#9c714f",
        }
      },
      {
        "name": "[JAVASCRIPT] Parameters",
        "scope": ["variable.parameter.js"],
        "settings": {
          "foreground": "#fab1f6",
          "fontStyle": "italic"
        }
      },
      {
        "name": "[JAVASCRIPT] Variable other",
        "scope": ["variable.other.object.js"],
        "settings": {
          "foreground": "#bf9e8d"
        }
      },
      {
        "name": "[JAVASCRIPT] Variable other property",
        "scope": ["variable.other.property.js"],
        "settings": {
          "foreground": "#fe7ed3"
        }
      },
      {
        "name": "[JAVASCRIPT] Variable other readwrite",
        "scope": ["variable.other.readwrite.js"],
        "settings": {
          "foreground": "#fccef9"
        }
      },
      {
        "name": "[JAVASCRIPT] Variable other object property ",
        "scope": ["variable.other.object.property.js"],
        "settings": {
          "foreground": "#925870"
        }
      },
      {
        "name": "[JAVASCRIPT] Arrow function",
        "scope": [
          "variable.other.readwrite.alias.js",
          "storage.type.function.arrow.js",
        ],
        "settings": {
          "foreground": "#d897f7",
        }
      },
      {
        "name": "[JAVASCRIPT] Import",
        "scope": "keyword.control.import.js",
        "settings": {
          "foreground": "#fe7ed3"
        }
      },
      {
        "name": "[JAVASCRIPT] From",
        "scope": "keyword.control.from.js",
        "settings": {
          "foreground": "#fe7ed3"
        }
      },
      {
        "name": "[JAVASCRIPT] Import all and Booleans",
        "scope": [
          "constant.language.import-export-all.js",
          "constant.language.boolean.true.js",
          "constant.language.boolean.false.js"
        ],
        "settings": {
          "foreground": "#d897f7"
        }
      },
      {
        "name": "[JAVASCRIPT] Strings and punctuation",
        "scope": [
          "string.quoted.single.js",
          "string.quoted.double.js",
          "string.template.js",
          "punctuation.definition.template-expression.begin.js",
          "punctuation.definition.template-expression.end.js",
          "punctuation.definition.string.template.end.js",
          "punctuation.definition.string.template.begin.js",
          "punctuation.definition.string.begin.js",
          "punctuation.definition.string.end.js"
        ],
        "settings": {
          "foreground": "#a4a3b3"
        }
      },
      {
        "name": "[JAVASCRIPT] Conditionals",
        "scope": "keyword.control.conditional.js",
        "settings": {
          "foreground": "#c2817f",
        }
      },
      // CSS Colors
      {
        "name": "[CSS] All selector",
        "scope":"entity.name.tag.wildcard.css",
        "settings": {
          "foreground": "#d2a1ff"
        }
      },
      {
        "name": "[CSS] Entity tag name",
        "scope": "entity.name.tag.css",
        "settings": {
          "foreground": "#b06c17",
        }
      },
      {
        "name": "[CSS] Property name",
        "scope": [
          "support.type.property-name.css",
          "support.type.vendored.property-name.css"
        ],
        "settings": {
          "foreground": "#f9ea83",
        }
      },
      {
        "name": "[CSS] ID selector",
        "scope": "entity.other.attribute-name.id.css",
        "settings": {
          "foreground": "#a72406"
        }
      },
      {
        "name": "[CSS] Class selector",
        "scope" : "entity.other.attribute-name.class.css",
        "settings": {
          "foreground": "#818eb8"
        }
      },
      {
        "name": "[CSS] Property value",
        "scope": [
          "support.constant.property-value.css",
          "constant.numeric.css",
          "string.quoted.single.css",
          "string.quoted.double.css",
          "punctuation.definition.string.begin.css",
          "punctuation.definition.string.end.css"
        ],
        "settings": {
          "foreground": "#cab49d"
        }
      },
      {
        "name": "[CSS] Units",
        "scope": [
          "keyword.other.unit.px.css",
          "keyword.other.unit.percentage.css",
          "keyword.other.unit.vh.css",
          "keyword.other.unit.rem.css",
          "keyword.other.unit.ms.css",
          "keyword.other.unit.s.css"
        ],
        "settings": {
          "foreground": "#cab49d"
        }
      },
      {
        "name": "[CSS] RGBA Value",
        "scope": [
          "constant.other.color.rgb-value.hex.css",
          "support.function.misc.css"
        ],
        "settings": {
          "foreground": "#cab49d"
        }
      },
      {
        "name": "[CSS] Other value",
        "scope": "meta.property-value.css",
        "settings": {
          "foreground": "#d4a36d",
        }
      },
      {
        "name": "[CSS] Pseudo class",
        "scope": "entity.other.attribute-name.pseudo-class.css",
        "settings": {
          "foreground": "#fcf0b6",
          "fontStyle": "italic"
        }
      },
      // XML Colors
      {
        "name": "[XML] Tag name",
        "scope": "entity.name.tag.localname.xml",
        "settings": {
          "foreground": "#ec635f"
        }
      },
      {
        "name": "[XML] Attributes",
        "scope": "entity.other.attribute-name.localname.xml",
        "settings": {
          "foreground": "#F0E0C0"
        }
      }
    ]
  },
}