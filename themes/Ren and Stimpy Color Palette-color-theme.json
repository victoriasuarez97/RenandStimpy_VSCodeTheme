  "editor.tokenColorCustomizations": {
    "textMateRules": [
      {
        "name": "Strings",
        "scope": [
          "string.quoted.double",
          "string.quoted.double punctuation",
          "string.quoted.single",
          "punctuation.definition.string.template.begin.js",
          "punctuation.definition.string.template.end.js",
          "string.template.js"
        ],
        "settings": {
          "foreground": "#80aef3"
        }
      },
      {
        "name": "Comment and Punctuation Settings",
        "scope": [
          "comment",
          "punctuation.definition.tag.begin.html",
          "punctuation.definition.tag.end.html"
        ],
        "settings": {
          "foreground": "#EB71B6",
          "fontStyle": "italic"
        }
      },
      {
        "name": "[HTML] Entity Tag Settings",
        "scope": ["entity.name.tag.html"],
        "settings": {
          "foreground": "#ec635f"
        }
      },
      {
        "name": "[HTML] Tag Attributes Settings",
        "scope": ["entity.other.attribute-name"],
        "settings": {
          "foreground": "#F0E0C0",
          "fontStyle": "italic"
        }
      },
      {
        "name": "[JSON] variable type",
        "scope": ["meta.var.expr.js"],
        "settings": {
          "foreground": "#F0E0C0"
        }
      },
      {
        "name": "[JSON] Properties",
        "scope": [
          "support.type.property-name.json",
          "support.type.property-name.json punctuation"
        ],
        "settings": {
          "foreground": "#a480f3"
        }
      },
      {
        "name": "[JAVASCRIPT] Type",
        "scope": ["storage.type.js"],
        "settings": {
          "foreground": "#EB71B6"
        }
      },
      {
        "name": "[JAVASCRIPT] Variable type",
        "scope": ["storage.type.js"],
        "settings": {
          "foreground": "#f9c0c6"
        }
      },
      {
        "name": "[JAVASCRIPT] Variable declared",
        "scope": ["variable.other.constant.js"],
        "settings": {
          "foreground": "#EB71B6"
        }
      },
      {
        "name": "[JAVASCRIPT] Support function",
        "scope": ["support.function.js"],
        "settings": {
          "foreground": "#f2a68c"
        }
      },
      {
        "name": "[JAVASCRIPT] Function",
        "scope": ["storage.type.function.js"],
        "settings": {
          "foreground": "#afa568",
          "fontStyle": "italic"
        }
      },
      {
        "name": "[JAVASCRIPT] Name of function",
        "scope": ["entity.name.function.js"],
        "settings": {
          "foreground": "#fce89f"
        }
      },
      {
        "name": "[JAVASCRIPT] Return function",
        "scope": ["keyword.control.flow.js"],
        "settings": {
          "foreground": "#97b99e"
        }
      },
      {
        "name": "[JAVASCRIPT] Module export",
        "scope": ["support.type.object.module.js"],
        "settings": {
          "foreground": "#A21511",
        }
      },
      {
        "name": "[JAVASCRIPT] Parameters",
        "scope": ["variable.parameter.js"],
        "settings": {
          "foreground": "#fab1f6",
          "fontStyle": "italic"
        }
      },
      {
        "name": "[JAVASCRIPT] Variable other",
        "scope": ["variable.other.object.js"],
        "settings": {
          "foreground": "#b3d3f9"
        }
      },
      {
        "name": "[JAVASCRIPT] Variable other property",
        "scope": ["variable.other.property.js"],
        "settings": {
          "foreground": "#fe7ed3"
        }
      },
      {
        "name": "[JAVASCRIPT] Variable other readwrite",
        "scope": ["variable.other.readwrite.js"],
        "settings": {
          "foreground": "#fccef9"
        }
      },
      {
        "name": "[JAVASCRIPT] Variable other object property ",
        "scope": ["variable.other.object.property.js"],
        "settings": {
          "foreground": "#e8fca1"
        }
      }
    ]
  },
}